////Use of an array to read a list of integers and output them *
//* in the reverse order of reading.
#include <iostream>
using namespace std;
int main(){

int arr[5];
cout<<" Enter the values for array :"<<endl;
for (int i=0; i<5; i++){
	cin>>arr[i];
}  
    cout<<" The values in array are "<<endl;
    
    for (int i=0; i<5; i++){
    	cout<<arr[i];
    	cout<<endl;
	}
	cout<<" till this it is good now move on to reverse "<<endl;
	cout<<" ::::::::::::::::::::::::::::::::::::::::::::"<<endl;
	cout<<" ::::::::::::::::::::::::::::::::::::::::::::"<<endl;
	cout<<" The values in reverse order are :"<<endl;
	  for (int i=4; i>=0; i--){
    	cout<<arr[i];
    	cout<<endl;
		}
	
	
	return 0;
	}
